{
    "name": "webnazakazku/nette-classes-standards",
    "type": "library",
    "description": "Better names of classes in Nette 3.1",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "nette/application": "^3.1",
        "nette/caching": "^3.1",
        "nette/component-model": "^3.0",
        "nette/database": "^3.1",
        "nette/di": "^3.0",
        "nette/forms": "^3.1",
        "nette/http": "^3.1",
        "nette/mail": "^3.1",
        "nette/security": "^3.1",
        "nette/utils": "^3.2",
        "latte/latte": "^2.9",
        "ninjify/qa": "^0.12",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-nette": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "nette/tester": "^2.3"
